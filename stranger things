
live_loop :doom1 do
  use_bpm 160
  sample :basic_hard_c, amp: 3
  sleep 3
end

live_loop :doom2 do
  use_bpm 160
  use_synth :prophet
  play :c4
  sleep 0.5
  play :e4
  sleep 0.5
  play :g4
  sleep 0.5
  play :b4
  sleep 0.5
  play :c5
  sleep 0.5
  play :b4
  sleep 0.5
  play :g4
  sleep 0.5
  play :e4
  sleep 0.5
end

